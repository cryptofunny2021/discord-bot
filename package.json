{
  "name": "discord-bot",
  "private": true,
  "version": "1.0.0",
  "repository": "TreasureProject/discord-bot",
  "type": "module",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "https://neilkistner.com"
  },
  "license": "MIT",
  "main": "build/src/index.js",
  "scripts": {
    "build": "yarn generate:graphql && tsc",
    "generate:graphql": "graphql-codegen -r dotenv/config",
    "generate:graphql:watch": "yarn generate:graphql --watch",
    "migrate": "prisma migrate deploy",
    "start": "node build/src/index.js",
    "dev": "nodemon --exec 'ts-node-esm -T' src/index.ts"
  },
  "dependencies": {
    "@discordx/importer": "1.1.10",
    "@discordx/utilities": "5.0.0",
    "date-fns": "2.29.2",
    "discord.js": "14.3.0",
    "discordx": "11.1.12",
    "dotenv": "16.0.2",
    "ethers": "5.7.0",
    "google-spreadsheet": "3.3.0",
    "got-scraping": "3.2.10",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "graphql-tag": "2.12.6",
    "p-queue": "7.3.0",
    "read-pkg": "7.1.0",
    "reflect-metadata": "0.1.13",
    "valtio": "1.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-graphql-request": "4.5.4",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@types/google-spreadsheet": "3.3.0",
    "@types/node": "18.7.15",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  }
}
